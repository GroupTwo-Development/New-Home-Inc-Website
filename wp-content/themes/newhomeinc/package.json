  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": "git+https://github.com/Automattic/_s.git",
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "devDependencies": {
    "@wordpress/scripts": "^12.1.0",
    "dir-archiver": "^1.1.1",
